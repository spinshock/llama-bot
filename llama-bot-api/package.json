{
  "name": "llama-bot-api",
  "version": "1.0.0",
  "description": "Llama-bot REST API with Express",
  "main": "index.js",
  "author": "Yavor Filipov",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js -e ts\"",
    "debug": "npm run build && concurrently --kill-others \"tsc -w\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "jest  --passWithNoTests",
    "test": "jest  --passWithNoTests",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "minimist": "1.2.5",
    "pg": "8.7.1",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "16.11.10",
    "concurrently": "6.4.0",
    "prettier": "2.5.0",
    "eslint": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "jest": "27.3.1",
    "nodemon": "2.0.15",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
